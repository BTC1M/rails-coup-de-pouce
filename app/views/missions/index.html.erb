<h1>All missions</h1>

<% @missions.each do |mission| %>
  <ul>
      <li>
          <%= link_to mission.title, mission_path(mission) %>
      </li>
  </ul>
<% end %>

<div
  id="map"
  style="width: 100%;
  height: 600px;"
  data-markers="<%= @markers.to_json %>"
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
></div>

<%#= render 'form', mission: @mission %>
